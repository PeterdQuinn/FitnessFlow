<div class="ui grid">
  <div class="eight wide column">
{{#if posts.length}}
<section>
  <h2>Your Posts</h2>
  
  <div class="ui feed">
  <ul>
    {{#each posts as | post|}}
    <li> 
      <div class="summary">
        <a class="user">
          {{user.username}}
        </a> 
        <div class="date">
          {{format_date created_at}}
        </div>
      </div>
      <a href="/dashboard-list/edit/{{post.id}}" class="edit-link">       
          <div class="card">
           <h3 class="card-header">{{title}}
          <span class="head-style">Posted by {{user.username}} on {{format_date created_at}}</span>
            </h3>
            
          </div>
    </a>
    </li>    
    {{user.id}} 
    {{user.username}}
    {{user.email}}  
    
      {{/each}}
  </ul>


  


</section>
{{/if}}

    <div style="text-align: center;">
      <a href="/dashboard">
    <button type="button" class="btn-post"><ion-icon name="add-outline"></ion-icon>New Post</button>
      </a>
    </div>
  </div>


  <div class="eight wide column">
    <a href='/workout-list'>
    <h2>Your Workouts</h2>
    
    </div>
  </div>
</div>





<script src="https://unpkg.com/ionicons@5.4.0/dist/ionicons.js"></script>

